<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:conv="clr-namespace:Forex.Wpf.Resources.Converters">

    <conv:BoolToTextConverter x:Key="BoolToTextConverter" />
    <conv:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <conv:StringIsNotEmptyConverter x:Key="StringIsNotEmptyConverter" />
    <conv:InverseBoolToBoolConverter x:Key="InverseBoolToBoolConverter" />
    <conv:InverseBoolToVisibilityConverter x:Key="InverseBoolToVisibilityConverter" />
    <conv:IntToStringConverter x:Key="IntToStringConverter" />
    <conv:ImageFallbackConverter x:Key="ImageFallbackConverter" />
    <conv:ToStringConverter x:Key="ToStringConverter" />

</ResourceDictionary>